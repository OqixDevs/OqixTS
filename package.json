    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "axios": "1.11.0",
    "cheerio": "1.1.2",
    "eslint": "9.32.0",
    "jest": "30.0.5",
    "jest-mock-extended": "4.0.0",
    "prettier": "3.6.2",
    "prisma": "6.13.0",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
