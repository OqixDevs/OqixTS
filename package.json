    "@typescript-eslint/parser": "5.62.0",
    "axios": "1.4.0",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.47.0",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
